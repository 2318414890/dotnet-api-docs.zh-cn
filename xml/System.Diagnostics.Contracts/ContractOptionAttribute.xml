        <trans-unit id="136" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Diagnostics.Contracts.ContractOptionAttribute.Value">
          <source>The value for the option.</source>
          <target state="translated">选项的值。</target>       </trans-unit>
