    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.Presentation.CutCopyPasteHelper">
          <source>Provides functions to cut, copy, and paste items.</source>
          <target state="translated">提供剪切、复制和粘贴项的功能。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.CanCopy(System.Activities.Presentation.EditingContext)">
          <source>The editing context to be verified.</source>
          <target state="translated">要验证的编辑上下文。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.CanCopy(System.Activities.Presentation.EditingContext)">
          <source>Verifies whether items can be copied from the specified editing context.</source>
          <target state="translated">验证是否能从指定的编辑上下文中复制项。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.CanCopy(System.Activities.Presentation.EditingContext)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the number of the selected objects is more than 0 and <ph id="ph2">&lt;paramref name="context" /&gt;</ph> is not <ph id="ph3">&lt;see langword="null" /&gt;</ph>; otherwise, <ph id="ph4">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果选定对象的数量大于 0 且 <ph id="ph2">&lt;paramref name="context" /&gt;</ph> 不为 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，则为 <ph id="ph3">&lt;see langword="null" /&gt;</ph>；否则为 <ph id="ph4">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.CanCut(System.Activities.Presentation.EditingContext)">
          <source>The editing context to be verified.</source>
          <target state="translated">要验证的编辑上下文。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.CanCut(System.Activities.Presentation.EditingContext)">
          <source>Verifies whether items can be cut from the specified editing context.</source>
          <target state="translated">验证是否能从指定的编辑上下文中剪切项。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.CanCut(System.Activities.Presentation.EditingContext)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the number of the selected objects is more than 0 and <ph id="ph2">&lt;paramref name="context" /&gt;</ph> is not <ph id="ph3">&lt;see langword="null" /&gt;</ph>; otherwise, <ph id="ph4">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果选定对象的数量大于 0 且 <ph id="ph2">&lt;paramref name="context" /&gt;</ph> 不为 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，则为 <ph id="ph3">&lt;see langword="null" /&gt;</ph>；否则为 <ph id="ph4">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.CanPaste(System.Activities.Presentation.EditingContext)">
          <source>The editing context for the paste operation.</source>
          <target state="translated">粘贴操作的编辑上下文。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.CanPaste(System.Activities.Presentation.EditingContext)">
          <source>Returns a Boolean that indicates whether the specified objects can be pasted.</source>
          <target state="translated">返回一个布尔值，该值指示是否可粘贴指定的对象。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.CanPaste(System.Activities.Presentation.EditingContext)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the number of the specified objects is more than 0 and not <ph id="ph2">&lt;see langword="null" /&gt;</ph>; otherwise, <ph id="ph3">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果指定对象的数量大于 0 且不为 <ph id="ph1">&lt;see langword="true" /&gt;</ph> 则为 <ph id="ph2">&lt;see langword="null" /&gt;</ph>； 否则为 <ph id="ph3">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.DoCopy(System.Activities.Presentation.EditingContext)">
          <source>The editing context from which items are copied.</source>
          <target state="translated">从中复制项的编辑上下文。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.DoCopy(System.Activities.Presentation.EditingContext)">
          <source>Copies the currently selected items from the specified editing context to the clipboard.</source>
          <target state="translated">将当前选定的项从指定的编辑上下文复制到剪贴板。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.DoCut(System.Activities.Presentation.EditingContext)">
          <source>The editing context from which items are removed.</source>
          <target state="translated">从中移除项的编辑上下文。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.DoCut(System.Activities.Presentation.EditingContext)">
          <source>Deletes the currently selected items from an editing context.</source>
          <target state="translated">从编辑上下文中删除当前选定的项。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.DoPaste(System.Activities.Presentation.EditingContext)">
          <source>The editing context to which items are pasted.</source>
          <target state="translated">将项粘贴到其中的编辑上下文。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.CutCopyPasteHelper.DoPaste(System.Activities.Presentation.EditingContext)">
          <source>Pastes items in the clipboard in a default location in the specified editing context.</source>
          <target state="translated">将剪贴板中的项粘贴在指定编辑上下文中的默认位置。</target>       </trans-unit>
