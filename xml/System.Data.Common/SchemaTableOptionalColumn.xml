    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Data.Common.SchemaTableOptionalColumn">
          <source>Describes optional column metadata of the schema for a database table.</source>
          <target state="translated">描述数据库表的架构的可选列元数据。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SchemaTableOptionalColumn.AutoIncrementSeed">
          <source>Specifies the value at which the series for new identity columns is assigned.</source>
          <target state="translated">指定新标识列的序列被分配的值。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SchemaTableOptionalColumn.AutoIncrementStep">
          <source>Specifies the increment between values in the identity column.</source>
          <target state="translated">指定标识列中的值之间的增量。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SchemaTableOptionalColumn.BaseCatalogName">
          <source>The name of the catalog associated with the results of the latest query.</source>
          <target state="translated">与最新的查询的结果相关联的目录的名称。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SchemaTableOptionalColumn.BaseColumnNamespace">
          <source>The namespace of the column.</source>
          <target state="translated">列的命名空间。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SchemaTableOptionalColumn.BaseServerName">
          <source>The server name of the column.</source>
          <target state="translated">列的服务器名。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SchemaTableOptionalColumn.BaseTableNamespace">
          <source>The namespace for the table that contains the column.</source>
          <target state="translated">包含列的表的命名空间。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SchemaTableOptionalColumn.ColumnMapping">
          <source>Specifies the mapping for the column.</source>
          <target state="translated">指定列的映射。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SchemaTableOptionalColumn.DefaultValue">
          <source>The default value for the column.</source>
          <target state="translated">列的默认值。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SchemaTableOptionalColumn.Expression">
          <source>The expression used to compute the column.</source>
          <target state="translated">用于计算该列的表达式。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SchemaTableOptionalColumn.IsAutoIncrement">
          <source>Specifies whether the column values in the column are automatically incremented.</source>
          <target state="translated">指定列中的列值是否自动递增。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="F:System.Data.Common.SchemaTableOptionalColumn.IsAutoIncrement">
          <source>Returns the string "true" if the column is automatically incremented; otherwise "false."</source>
          <target state="translated">如果自动递增列;，返回字符串"true"否则为"false。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SchemaTableOptionalColumn.IsHidden">
          <source>Specifies whether this column is hidden.</source>
          <target state="translated">指定该列是否是隐藏的。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" extradata="MT" uid="F:System.Data.Common.SchemaTableOptionalColumn.IsHidden">
          <source>Returns the string "true" if the column is hidden, otherwise "false."</source>
          <target state="translated">如果，返回字符串"true"隐藏列，否则"false。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SchemaTableOptionalColumn.IsReadOnly">
          <source>Specifies whether this column is read-only.</source>
          <target state="translated">指定该列是否是只读的。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" extradata="MT" uid="F:System.Data.Common.SchemaTableOptionalColumn.IsReadOnly">
          <source>Returns the string "true" if the column is read-only, otherwise "false."</source>
          <target state="translated">返回字符串"true"如果列是只读的否则"false。</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SchemaTableOptionalColumn.IsRowVersion">
          <source>Specifies whether this column contains row version information.</source>
          <target state="translated">指定该列是否包含行版本信息。</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve" extradata="MT" uid="F:System.Data.Common.SchemaTableOptionalColumn.IsRowVersion">
          <source>Returns the string "true" if the column contains row version information, otherwise "false."</source>
          <target state="translated">返回字符串"true"如果列包含行版本信息，否则"false。</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve" uid="F:System.Data.Common.SchemaTableOptionalColumn.ProviderSpecificDataType">
          <source>Specifies the provider-specific data type of the column.</source>
          <target state="translated">指定列的提供程序特定的数据类型。</target>       </trans-unit>
      </group>
