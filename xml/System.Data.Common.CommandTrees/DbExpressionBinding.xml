          <target state="translated">获取引用元素变量的 <ph id="ph1">&lt;see cref="T:System.Data.Common.CommandTrees.DbVariableReferenceExpression" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.Common.CommandTrees.DbExpressionBinding.Variable">
          <source>The variable reference.</source>
          <target state="translated">变量引用。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="P:System.Data.Common.CommandTrees.DbExpressionBinding.VariableName">
          <source>Gets the name assigned to the element variable.</source>
          <target state="translated">获取指定给元素变量的名称。</target>       </trans-unit>
