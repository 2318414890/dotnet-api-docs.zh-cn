      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Data.DataTransferEventArgs">
          <source>Encapsulates arguments for data transfer events.</source>
          <target state="translated">封装数据传输事件的参数。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Data.DataTransferEventArgs">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Data.Binding.TargetUpdated&gt;</ph> event occurs when a value is transferred from the binding source to the binding target, but only for bindings with the <ph id="ph2">&lt;xref:System.Windows.Data.Binding.NotifyOnTargetUpdated%2A&gt;</ph> value set to <ph id="ph3">`true`</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Windows.Data.Binding.TargetUpdated&gt;</ph>事件发生时使用的绑定到绑定目标，但仅限于一个值从绑定源传输<ph id="ph2">&lt;xref:System.Windows.Data.Binding.NotifyOnTargetUpdated%2A&gt;</ph>值设置为<ph id="ph3">`true`</ph>。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Data.DataTransferEventArgs">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Data.Binding.SourceUpdated&gt;</ph> event occurs when a value is transferred from the target to the source, but only for bindings with the <ph id="ph2">&lt;xref:System.Windows.Data.Binding.NotifyOnSourceUpdated%2A&gt;</ph> value set to <ph id="ph3">`true`</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Windows.Data.Binding.SourceUpdated&gt;</ph>事件发生时当值从目标到源，但仅针对与具有绑定<ph id="ph2">&lt;xref:System.Windows.Data.Binding.NotifyOnSourceUpdated%2A&gt;</ph>值设置为<ph id="ph3">`true`</ph>。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Windows.Data.DataTransferEventArgs.InvokeEventHandler(System.Delegate,System.Object)">
          <source>The generic handler to call in a type-specific way.</source>
          <target state="translated">以特定于类型的方式调用的一般处理程序。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Windows.Data.DataTransferEventArgs.InvokeEventHandler(System.Delegate,System.Object)">
          <source>The object to invoke the handler on.</source>
          <target state="translated">要对其调用处理程序的对象。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Windows.Data.DataTransferEventArgs.InvokeEventHandler(System.Delegate,System.Object)">
          <source>Invokes the specified handler in a type-specific way on the specified object.</source>
          <target state="translated">对指定对象以特定于类型的方式调用指定的处理程序。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.Windows.Data.DataTransferEventArgs.Property">
          <source>Gets the specific binding target property that is involved in the data transfer event.</source>
          <target state="translated">获取数据传输事件所涉及的特定绑定目标属性。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Data.DataTransferEventArgs.Property">
          <source>The property that changed.</source>
          <target state="translated">已更改的属性。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="P:System.Windows.Data.DataTransferEventArgs.TargetObject">
          <source>Gets the binding target object of the binding that raised the event.</source>
          <target state="translated">获取绑定中引发了事件的绑定目标对象。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Windows.Data.DataTransferEventArgs.TargetObject">
          <source>The target object of the binding that raised the event.</source>
          <target state="translated">绑定中引发了事件的目标对象。</target>       </trans-unit>
