      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.Presentation.View.ViewStateChangedEventArgs">
          <source>Provides an object formatted to serve as an event argument containing the information about the changed view state.</source>
          <target state="translated">提供一个用作事件自变量的格式化对象，其中包含有关已更改视图状态的信息。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Activities.Presentation.View.ViewStateChangedEventArgs">
          <source>View state changes are changes made to a workflow that impact layout, such as positioning an element in a flowchart, or expanding and collapsing a container in a sequence.</source>
          <target state="translated">视图状态更改是指对影响布局的工作流所做的更改，例如：挪动流程图中的某个元素，或者展开和折叠序列中的容器。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.View.ViewStateChangedEventArgs.#ctor(System.Activities.Presentation.Model.ModelItem,System.String,System.Object,System.Object)">
          <source>An item in the activity designer model that serves as a parent element in the XML view of the model.</source>
          <target state="translated">活动设计器模型中的一个项，该项在模型的 XML 视图中用作父元素。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.View.ViewStateChangedEventArgs.#ctor(System.Activities.Presentation.Model.ModelItem,System.String,System.Object,System.Object)">
          <source>The name of an attribute in the specified parent element.</source>
          <target state="translated">指定父元素中的特性的名称。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.View.ViewStateChangedEventArgs.#ctor(System.Activities.Presentation.Model.ModelItem,System.String,System.Object,System.Object)">
          <source>The new value for the specified attribute key.</source>
          <target state="translated">指定的特性键的新值。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.View.ViewStateChangedEventArgs.#ctor(System.Activities.Presentation.Model.ModelItem,System.String,System.Object,System.Object)">
          <source>The old value of the specified attribute key.</source>
          <target state="translated">指定的特性键的旧值。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.View.ViewStateChangedEventArgs.#ctor(System.Activities.Presentation.Model.ModelItem,System.String,System.Object,System.Object)">
          <source>Creates a new instance of the <ph id="ph1">&lt;see cref="T:System.Activities.Presentation.View.ViewStateChangedEventArgs" /&gt;</ph> class with a specified parent model item and its key/value pair with the old and the new values.</source>
          <target state="translated">用指定的父模型项以及具有旧值和新值的键/值对来创建 <ph id="ph1">&lt;see cref="T:System.Activities.Presentation.View.ViewStateChangedEventArgs" /&gt;</ph> 类的新实例。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.Activities.Presentation.View.ViewStateChangedEventArgs.Key">
          <source>Gets an attribute key name in a specified view element.</source>
          <target state="translated">从指定的视图元素中获取一个特性键名称。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Presentation.View.ViewStateChangedEventArgs.Key">
          <source>A string that represents the name of a key in a view element.</source>
          <target state="translated">一个表示视图元素中的键名的字符串。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:System.Activities.Presentation.View.ViewStateChangedEventArgs.NewValue">
          <source>Gets the new value for the key in the current attribute key/value pair.</source>
          <target state="translated">为当前特性键/值对中的键获取新值。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Presentation.View.ViewStateChangedEventArgs.NewValue">
          <source>An object that contains a key value.</source>
          <target state="translated">一个包含键值的对象。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="P:System.Activities.Presentation.View.ViewStateChangedEventArgs.OldValue">
          <source>Gets the old value of the key in the current attribute key/value pair.</source>
          <target state="translated">获取当前特性键/值对中的键的旧值。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Presentation.View.ViewStateChangedEventArgs.OldValue">
          <source>An object that contains a key value.</source>
          <target state="translated">一个包含键值的对象。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="P:System.Activities.Presentation.View.ViewStateChangedEventArgs.ParentModelItem">
          <source>Gets an item in the activity designer model that serves as a parent item in the XAML view of the model.</source>
          <target state="translated">从活动设计器模型中获取一项，该项在模型的 XAML 视图中用作父项。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Presentation.View.ViewStateChangedEventArgs.ParentModelItem">
          <source>A parent element in an activity designer model.</source>
          <target state="translated">活动设计器模型中的父元素。</target>       </trans-unit>
      </group>
