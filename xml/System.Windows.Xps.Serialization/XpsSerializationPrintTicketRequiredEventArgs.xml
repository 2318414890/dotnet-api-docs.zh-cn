        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs">
          <source>Provides data for the <ph id="ph1">&lt;see cref="E:System.Windows.Xps.Serialization.XpsSerializationManager.XpsSerializationPrintTicketRequired" /&gt;</ph> event.</source>
          <target state="translated">为 <ph id="ph1">&lt;see cref="E:System.Windows.Xps.Serialization.XpsSerializationManager.XpsSerializationPrintTicketRequired" /&gt;</ph> 事件提供数据。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Windows.Xps.Serialization.XpsSerializationPrintTicketRequiredEventArgs.#ctor(System.Windows.Xps.Serialization.PrintTicketLevel,System.Int32)">
