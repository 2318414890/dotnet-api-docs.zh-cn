      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Reflection.ParameterAttributes">
          <source>Defines the attributes that can be associated with a parameter.</source>
          <target state="translated">定义可以与参数相关联的属性。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.Reflection.ParameterAttributes">
          <source>These are defined in CorHdr.h.</source>
          <target state="translated">这些属性在 CorHdr.h 中进行定义。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Reflection.ParameterAttributes">
          <source>To get the <ph id="ph1">`ParameterAttributes`</ph> value, first get the <ph id="ph2">`Type`</ph>.</source>
          <target state="translated">若要获取<ph id="ph1">`ParameterAttributes`</ph>值时，首先获取<ph id="ph2">`Type`</ph>。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Reflection.ParameterAttributes">
          <source>From the <ph id="ph1">`Type`</ph>, get the <ph id="ph2">`ParameterInfo`</ph> array.</source>
          <target state="translated">从<ph id="ph1">`Type`</ph>，获取<ph id="ph2">`ParameterInfo`</ph>数组。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Reflection.ParameterAttributes">
          <source>The <ph id="ph1">`ParameterAttributes`</ph> value is within the array.</source>
          <target state="translated"><ph id="ph1">`ParameterAttributes`</ph>值是在该数组中。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Reflection.ParameterAttributes">
          <source>These enumerator values are dependent on optional metadata.</source>
          <target state="translated">这些枚举值是依赖于可选元数据。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Reflection.ParameterAttributes">
          <source>Not all attributes are available from all compilers.</source>
          <target state="translated">并非所有属性都将所有编译器中可用。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Reflection.ParameterAttributes">
          <source>See the appropriate compiler instructions to determine which enumerated values are available.</source>
          <target state="translated">请参阅相应的编译器指令，若要确定哪些枚举的值可用。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Reflection.ParameterAttributes">
          <source>The following example displays the attributes of the specified parameter.</source>
          <target state="translated">下面的示例显示了指定的参数的属性。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Reflection.ParameterAttributes.HasDefault">
          <source>Specifies that the parameter has a default value.</source>
          <target state="translated">指定参数具有默认值。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.Reflection.ParameterAttributes.HasFieldMarshal">
          <source>Specifies that the parameter has field marshaling information.</source>
          <target state="translated">指定该参数具有字段封送处理信息。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="F:System.Reflection.ParameterAttributes.In">
          <source>Specifies that the parameter is an input parameter.</source>
          <target state="translated">指定该参数是一个输入参数。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="F:System.Reflection.ParameterAttributes.Lcid">
          <source>Specifies that the parameter is a locale identifier (lcid).</source>
          <target state="translated">指定该参数是一个区域设置标识符 (lcid)。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="F:System.Reflection.ParameterAttributes.None">
          <source>Specifies that there is no parameter attribute.</source>
          <target state="translated">指定不存在参数属性。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="F:System.Reflection.ParameterAttributes.Optional">
          <source>Specifies that the parameter is optional.</source>
          <target state="translated">指定参数为可选。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="F:System.Reflection.ParameterAttributes.Out">
          <source>Specifies that the parameter is an output parameter.</source>
          <target state="translated">指定该参数是一个输出参数。</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="F:System.Reflection.ParameterAttributes.Reserved3">
          <source>Reserved.</source>
          <target state="translated">保留。</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve" uid="F:System.Reflection.ParameterAttributes.Reserved4">
          <source>Reserved.</source>
          <target state="translated">保留。</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve" uid="F:System.Reflection.ParameterAttributes.ReservedMask">
          <source>Specifies that the parameter is reserved.</source>
          <target state="translated">指定该参数是保留的。</target>       </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve" uid="F:System.Reflection.ParameterAttributes.Retval">
          <source>Specifies that the parameter is a return value.</source>
          <target state="translated">指定该参数是一个返回值。</target>       </trans-unit>
