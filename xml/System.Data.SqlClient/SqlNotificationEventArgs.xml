      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Data.SqlClient.SqlNotificationEventArgs">
          <source>Represents the set of arguments passed to the notification event handler.</source>
          <target state="translated">表示传递给通知事件处理程序的参数集。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Data.SqlClient.SqlNotificationEventArgs.#ctor(System.Data.SqlClient.SqlNotificationType,System.Data.SqlClient.SqlNotificationInfo,System.Data.SqlClient.SqlNotificationSource)">
          <source><ph id="ph1">&lt;see cref="T:System.Data.SqlClient.SqlNotificationType" /&gt;</ph> value that indicates whether this notification is generated because of an actual change, or by the subscription.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Data.SqlClient.SqlNotificationType" /&gt;</ph> 值，指示此通知是由于实际更改生成的还是通过订阅生成的。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Data.SqlClient.SqlNotificationEventArgs.#ctor(System.Data.SqlClient.SqlNotificationType,System.Data.SqlClient.SqlNotificationInfo,System.Data.SqlClient.SqlNotificationSource)">
          <source><ph id="ph1">&lt;see cref="T:System.Data.SqlClient.SqlNotificationInfo" /&gt;</ph> value that indicates the reason for the notification event.</source>
          <target state="translated">指示通知事件的原因的 <ph id="ph1">&lt;see cref="T:System.Data.SqlClient.SqlNotificationInfo" /&gt;</ph> 值。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Data.SqlClient.SqlNotificationEventArgs.#ctor(System.Data.SqlClient.SqlNotificationType,System.Data.SqlClient.SqlNotificationInfo,System.Data.SqlClient.SqlNotificationSource)">
          <source>This may occur because the data in the store actually changed, or the notification became invalid (for example, it timed out).</source>
          <target state="translated">发生的原因可能是：实际更改了存储区中的数据，或者通知变为无效（例如，它已超时）。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Data.SqlClient.SqlNotificationEventArgs.#ctor(System.Data.SqlClient.SqlNotificationType,System.Data.SqlClient.SqlNotificationInfo,System.Data.SqlClient.SqlNotificationSource)">
          <source><ph id="ph1">&lt;see cref="T:System.Data.SqlClient.SqlNotificationSource" /&gt;</ph> value that indicates the source that generated the notification.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Data.SqlClient.SqlNotificationSource" /&gt;</ph> 值，指示生成通知的源。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Data.SqlClient.SqlNotificationEventArgs.#ctor(System.Data.SqlClient.SqlNotificationType,System.Data.SqlClient.SqlNotificationInfo,System.Data.SqlClient.SqlNotificationSource)">
          <source>Creates a new instance of the <ph id="ph1">&lt;see cref="T:System.Data.SqlClient.SqlNotificationEventArgs" /&gt;</ph> object.</source>
          <target state="translated">创建 <ph id="ph1">&lt;see cref="T:System.Data.SqlClient.SqlNotificationEventArgs" /&gt;</ph> 对象的新实例。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.Data.SqlClient.SqlNotificationEventArgs.Info">
          <source>Gets a value that indicates the reason for the notification event, such as a row in the database being modified or a table being truncated.</source>
          <target state="translated">获取指示通知事件的原因（如数据库中的某个行已被修改或某个表已被截断）的值。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.SqlClient.SqlNotificationEventArgs.Info">
          <source>The notification event reason.</source>
          <target state="translated">通知事件原因。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.SqlClient.SqlNotificationEventArgs.Info">
          <source>This event may occur because the data in the store actually changed, or the notification is no longer valid (for example, it timed out).</source>
          <target state="translated">因为实际更改存储区中的数据，或者通知将不再有效 （例如，它操作已超时），则可能发生此事件。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:System.Data.SqlClient.SqlNotificationEventArgs.Source">
          <source>Gets a value that indicates the source that generated the notification, such as a change to the query data or the database’s state.</source>
          <target state="translated">获取指示生成通知（如查询数据或数据库状态已更改）的源的值。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.SqlClient.SqlNotificationEventArgs.Source">
          <source>The source of the notification.</source>
          <target state="translated">通知的源。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="P:System.Data.SqlClient.SqlNotificationEventArgs.Type">
          <source>Gets a value that indicates whether this notification is generated because of an actual change, or by the subscription.</source>
          <target state="translated">获取一个值，该值指示此通知是由于实际更改生成的还是通过订阅生成的。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Data.SqlClient.SqlNotificationEventArgs.Type">
          <source>A value indicating whether the notification was generated by a change or a subscription.</source>
          <target state="translated">指示通知是否由更改或订阅生成的值。</target>       </trans-unit>
