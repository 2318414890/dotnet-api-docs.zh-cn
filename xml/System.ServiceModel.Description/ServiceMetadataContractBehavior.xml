        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Description.ServiceMetadataContractBehavior.MetadataGenerationDisabled">
          <source>Gets or sets a value that specifies whether metadata generation should be disabled for the endpoint.</source>
          <target state="translated">获取或设置一个值，该值指定是否应对终结点禁用元数据生成。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Description.ServiceMetadataContractBehavior.MetadataGenerationDisabled">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if metadata generation should be disabled; otherwise <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果应禁用元数据生成，则为 <ph id="ph1">&lt;see langword="true" /&gt;</ph>；否则为 <ph id="ph2">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
